{
  "consumer": {
    "name": "frontend"
  },
  "provider": {
    "name": "backend"
  },
  "interactions": [
    {
      "description": "a request to get an EAN8 barcode",
      "request": {
        "method": "POST",
        "path": "/barcode/ean8",
        "headers": {
          "Accept": "image/svg+xml",
          "Content-Type": "text/plain"
        },
        "body": "0000000"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "image/svg+xml; charset=utf-8"
        },
        "body": "<svg></svg>",
        "matchingRules": {
          "$.body": {
            "match": "regex",
            "regex": "<svg.*<\\/svg>$"
          }
        }
      }
    },
    {
      "description": "a request to get an EAN13 barcode",
      "request": {
        "method": "POST",
        "path": "/barcode/ean13",
        "headers": {
          "Accept": "image/svg+xml",
          "Content-Type": "text/plain"
        },
        "body": "012345678901"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "image/svg+xml; charset=utf-8"
        },
        "body": "<svg></svg>",
        "matchingRules": {
          "$.body": {
            "match": "regex",
            "regex": "<svg.*<\\/svg>$"
          }
        }
      }
    },
    {
      "description": "a request to get an EAN13 binary code",
      "request": {
        "method": "POST",
        "path": "/barcode/ean13",
        "headers": {
          "Accept": "text/plain",
          "Content-Type": "text/plain"
        },
        "body": "012345678901"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "text/plain; charset=utf-8"
        },
        "body": "101 0001101 0001101 0001101 0001101 0001101 0001101 01010 1110010 1110010 1110010 1110010 1110010 1110010 101",
        "matchingRules": {
          "$.body": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "a request to get an EAN8 binary code",
      "request": {
        "method": "POST",
        "path": "/barcode/ean8",
        "headers": {
          "Accept": "text/plain",
          "Content-Type": "text/plain"
        },
        "body": "0000000"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "text/plain; charset=utf-8"
        },
        "body": "101 0001101 0001101 0001101 0001101 0001101 0001101 01010 1110010 1110010 1110010 1110010 1110010 1110010 101",
        "matchingRules": {
          "$.body": {
            "match": "regex",
            "regex": "^[01 ]+$"
          }
        }
      }
    },
    {
      "description": "a request to get an CODE39 barcode",
      "request": {
        "method": "POST",
        "path": "/barcode/code39",
        "headers": {
          "Accept": "image/svg+xml",
          "Content-Type": "text/plain"
        },
        "body": "ABCDEFGHIJKLMNOPQRSTUVWXZ01234567890$./ -%+"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "image/svg+xml; charset=utf-8"
        },
        "body": "<svg></svg>",
        "matchingRules": {
          "$.body": {
            "match": "regex",
            "regex": "<svg.*<\\/svg>$"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}